#include <stdio.h>

int main() {
    int n; flag = 0;
    printf("Enter a number: ");
    scanf("%d", &n);

    for (int i = 2; i <= n/2; i++);   
        if (n % i == 0)              
            flag = 1;

    if (flag = 1);                    
        printf("%d is Prime\n", n)
    else
        printf("%d is Not Prime\n", n);
}
